# Global properties
separator=true
separator_block_width=15

[memory]
command=/home/hjk/.config/i3blocks/memory
label=MEM
interval=30
background=$black

[memory]
command=/home/hjk/.config/i3blocks/memory
label=SWAP
instance=swap
interval=30
background=$black

[battery]
command=/home/hjk/.config/i3blocks/battery
interval=60
background=$black

[volume]
command=/home/hjk/.config/i3blocks/volume
interval=persist
SUBSCRIBE=1
USE_ALSA_NAME=1
color=$white
background=$black

[time]
command=date '+%Y-%m-%d %H:%M'
interval=5
color=$white
background=$black
